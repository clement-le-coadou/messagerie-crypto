[ req ]
default_bits        = 2048
default_keyfile     = server.key
distinguished_name  = req_distinguished_name
x509_extensions     = v3_ca
prompt              = no

[ req_distinguished_name ]
C = FR
ST = Paris
L = Paris
O = Mon Entreprise
OU = Mon Application
CN = localhost

[ v3_ca ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = localhost
